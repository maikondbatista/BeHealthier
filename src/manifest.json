{
  "name": "Be Healthier",
  "version": "1.0",
  "description": "Build an Extension with Angular",
  "manifest_version": 2,
  "browser_action": {
    "browser_style": true,
    "default_title": "Be Healthier",
    "default_popup": "index.html?#/settings"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["notifications", "alarms", "background"]
}
